  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jasmine All",
      "program": "${workspaceFolder}/node_modules/.bin/tsx",
      "args": ["node_modules/jasmine/bin/jasmine.js"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jasmine Current",
      "program": "${workspaceFolder}/node_modules/.bin/tsx",
      "args": ["node_modules/jasmine/bin/jasmine.js", "${file}"],
      "console": "integratedTerminal"
    }
  ]
}
